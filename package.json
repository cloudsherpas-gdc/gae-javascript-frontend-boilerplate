{
  "name": "gae-javascript-frontend-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate code for Angular apps using Webpack and Babel",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudsherpas-gdc/gae-javascript-frontend-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "webpack",
    "babel"
  ],
  "author": "Ben Sarmiento <me@bensarmiento.com> (http://github.com/yowmamasita)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/cloudsherpas-gdc/gae-javascript-frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/cloudsherpas-gdc/gae-javascript-frontend-boilerplate",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "dependencies": {
    "angular": "^1.4.9"
  }
}
